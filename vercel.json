{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "buildCommand": "composer install --no-dev --optimize-autoloader && npm run build",
  "installCommand": "npm install",
  "outputDirectory": "public"
}